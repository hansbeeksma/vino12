{
  "name": "vino12",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "db:types": "supabase gen types typescript --local > src/lib/database.types.ts",
    "db:migrate": "supabase db push",
    "db:diff": "supabase db diff -f",
    "db:reset": "supabase db reset",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "services:up": "docker compose up -d",
    "services:down": "docker compose down",
    "services:logs": "docker compose logs -f",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org $SENTRY_ORG --project vino12 ./.next",
    "tunnel": "ngrok http 3000",
    "search:reindex": "npm run typesense:setup && npm run typesense:index",
    "lhci:autorun": "lhci autorun --config ./lighthouserc.json",
    "typesense:setup": "npx tsx scripts/typesense-setup.ts",
    "typesense:index": "npx tsx scripts/typesense-index-wines.ts",
    "prepare": "husky",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write ."
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@mojs/core": "^1.3.0",
    "@mollie/api-client": "^4.4.0",
    "@react-email/components": "^1.0.7",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@sentry/nextjs": "^10.38.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tensorflow-models/mobilenet": "^2.1.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@theatre/core": "^0.7.2",
    "@theatre/r3f": "^0.7.2",
    "@theatre/studio": "^0.7.2",
    "@tsparticles/engine": "^3.9.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.9.1",
    "@types/matter-js": "^0.20.2",
    "@upstash/redis": "^1.36.2",
    "@vercel/otel": "^2.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "granim": "^2.0.0",
    "lucide-react": "^0.563.0",
    "matter-js": "^0.20.0",
    "motion": "^12.33.0",
    "next": "^16.1.6",
    "proton-engine": "^7.1.5",
    "qrcode": "^1.5.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "resend": "^6.9.1",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "tw-animate-css": "^1.4.0",
    "typesense": "^3.0.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@lhci/cli": "^0.15.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.10",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
